doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no')
    title index
  style.
    *{
    margin: 0;
    padding: 0;
    }
  body
    .pagelist
      each val in title
        a(href='#{val}.html') #{val}